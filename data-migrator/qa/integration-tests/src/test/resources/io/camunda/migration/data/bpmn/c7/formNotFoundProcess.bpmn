<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
                  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
                  xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC"
                  xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI"
                  xmlns:camunda="http://camunda.org/schema/1.0/bpmn"
                  id="Definitions_FormNotFound"
                  targetNamespace="http://camunda.org/examples">

  <bpmn:process id="formNotFoundProcessId"
                name="Form Not Found Incident Test"
                isExecutable="true"
                camunda:historyTimeToLive="1">

    <!-- Start event with a start form key pointing to a non-existent form resource -->
    <bpmn:startEvent id="start"
                     camunda:formKey="embedded:deployment:org/camunda/bpm/engine/test/api/form/start.html">
      <bpmn:outgoing>flow_start_task</bpmn:outgoing>
    </bpmn:startEvent>

    <bpmn:sequenceFlow id="flow_start_task"
                       sourceRef="start"
                       targetRef="checkFormTask"/>

    <!-- Async service task that will try to load the deployed start form -->
    <bpmn:serviceTask id="checkFormTask"
                      name="Check Deployed Start Form"
                      camunda:class="io.camunda.migration.data.qa.history.entity.delegate.MissingFormDelegate"
                      camunda:asyncBefore="true">
      <bpmn:incoming>flow_start_task</bpmn:incoming>
      <bpmn:outgoing>flow_task_end</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:sequenceFlow id="flow_task_end"
                       sourceRef="checkFormTask"
                       targetRef="end"/>

    <bpmn:endEvent id="end">
      <bpmn:incoming>flow_task_end</bpmn:incoming>
    </bpmn:endEvent>

  </bpmn:process>

  <bpmndi:BPMNDiagram id="BPMNDiagram_FormNotFound">
    <bpmndi:BPMNPlane id="BPMNPlane_FormNotFound" bpmnElement="formNotFoundProcess"/>
  </bpmndi:BPMNDiagram>

</bpmn:definitions>